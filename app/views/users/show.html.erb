<div class="row">
  <div class="col-md-6 offset-3">
        <h2 class="h5 m-0 p-0 d-flex align-items-center">
          <%= image_tag current_user.avatar_image_url, class: "rounded-circle me-2", width: "200px"%>
          <%= current_user.username %>
        </h2>

    <ul class="nav nav-pills nav-justified">
      <li class="nav-item">
        <%= link_to "Posts", user_path(@user.username), class: "nav-link" %>
      </li>
      <li class="nav-item">
        <%= link_to "Liked", liked_path(@user.username), class: "nav-link" %>
      </li>
    </ul>
  </div>
</div>

<% @user.own_photos.each do |photo| %>
  <%= render "photos/photo", photo: photo %>
<% end %>
